{
  "name": "nextjs-template",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "set-env": "export $(cat .production.env | grep \"^[^#;]\" |xargs)",
    "set-env:dev": "export $(cat .env | grep \"^[^#;]\" |xargs)",
    "set-env:test": "export $(cat .test.env | grep \"^[^#;]\" |xargs)",
    "dev": "yarn set-env:dev && next dev -p ${PORT:-3000}",
    "build": "next build",
    "start": "yarn set-env && next start -p ${PORT:-3000}",
    "test": "NODE_PATH=./ SASS_PATH=node_modules:utils/styles jest --env=jsdom --detectOpenHandles",
    "lint": "eslint --fix . --ignore-path ./.gitignore",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc --ignore-path ./.gitignore",
    "storybook": "yarn set-env:dev && start-storybook -p 9009 -s public",
    "sb": "yarn set-env:dev && start-storybook -p 9009 -s public",
    "build-storybook": "yarn set-env && build-storybook -s public",
    "creator": "node ./bin/create/index.js",
    "creator:icons": "node --trace-uncaught node_modules/@svgr/cli/bin/svgr -d ./components/icons ./docs/icons",
    "prepare": "husky install",
    "set-env:dev-win": "for /F %A in (.env) do SET %A",
    "dev-win": "yarn set-env:dev-win && next dev -p ${PORT:-3000}",
    "sb-win": "yarn set-env:dev-win && start-storybook -p 9009 -s public"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@storybook/preset-typescript": "^3.0.0",
    "dotenv": "^10.0.0",
    "inquirer": "^8.2.0",
    "isomorphic-unfetch": "^3.1.0",
    "next": "9.3.0",
    "node-sass": "^4.14.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "sass": "^1.27.0",
    "swr": "^0.3.5"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@storybook/addon-a11y": "^6.3.12",
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-docs": "^6.3.12",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/builder-webpack5": "^6.3.12",
    "@storybook/manager-webpack5": "^6.3.12",
    "@storybook/node-logger": "^6.3.12",
    "@storybook/preset-create-react-app": "^4.0.0",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.3.12",
    "@svgr/cli": "^5.4.0",
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.0.2",
    "@types/node": "^14.6.4",
    "@types/react": "^16.9.49",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "@zeit/next-sass": "^1.0.1",
    "babel-loader": "^8.1.0",
    "babel-preset-react-app": "^9.1.2",
    "copy-dir": "^1.3.0",
    "css-loader": "^6.5.0",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.2",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^7.0.4",
    "jest": "^26.4.2",
    "jest-css-modules-transform": "^4.0.1",
    "jest-dom": "^4.0.0",
    "lint-staged": "^11.2.6",
    "prettier": "^2.4.1",
    "replace-terms-in-files": "^1.0.3",
    "sass-loader": "^10.0.3",
    "storybook-addon-designs": "^6.2.0",
    "storybook-addon-jsx": "^7.3.14",
    "style-loader": "^2.0.0",
    "ts-jest": "^26.4.3",
    "typescript": "^4.0.2"
  }
}
